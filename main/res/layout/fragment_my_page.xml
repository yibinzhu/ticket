<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.my.MyPage">

    <LinearLayout
        android:id="@+id/tab_ticket_book"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:paddingTop="10dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="56dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:tint="@color/t1_blue"
                android:src="@drawable/ic_my_canlendar_t_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginLeft="40dp"
                android:textSize="15sp"
                android:text="티켓북"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:tint="@color/t1_dark_gray"
                android:layout_alignParentRight="true"
                android:src="@drawable/ic_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tab_gift"
        android:layout_marginBottom="8dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="56dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:tint="@color/t1_blue"
                android:src="@drawable/ic_my_gift_t_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginLeft="40dp"
                android:textSize="15sp"
                android:text="선물하기"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:tint="@color/t1_dark_gray"
                android:layout_alignParentRight="true"
                android:src="@drawable/ic_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tab_parking"
        android:layout_marginBottom="8dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="56dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:tint="@color/t1_blue"
                android:src="@drawable/ic_my_car_t_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginLeft="40dp"
                android:textSize="15sp"
                android:text="주차권"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:tint="@color/t1_dark_gray"
                android:layout_alignParentRight="true"
                android:src="@drawable/ic_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tab_setting"
        android:layout_marginBottom="8dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="56dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:tint="@color/t1_blue"
                android:src="@drawable/ic_user_t_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginLeft="40dp"
                android:textSize="15sp"
                android:text="설정"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:tint="@color/t1_dark_gray"
                android:layout_alignParentRight="true"
                android:src="@drawable/ic_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tab_ticket_requesting"
        android:layout_marginBottom="8dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:background="@color/colorWhite"
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="56dp">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:tint="@color/t1_blue"
                android:src="@drawable/ic_receipt_t_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_marginLeft="40dp"
                android:textSize="15sp"
                android:text="타 예매처 발권요청"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:tint="@color/t1_dark_gray"
                android:layout_alignParentRight="true"
                android:src="@drawable/ic_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>